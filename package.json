{
  "name": "@rqjs/rqthemes",
  "version": "0.5.0",
  "description": "ricequant themes",
  "main": "index.css",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.ricequant.com:7999/node/ricequant-themes.git"
  },
  "author": "ansonw",
  "license": "MIT",
  "scripts": {
    "build": "./scripts/updep.sh && parcel build src/index.scss --no-cache --no-minify --no-source-maps --no-content-hash -d ./ && LEGACY=true parcel build src/index.scss --no-cache --no-minify --no-source-maps --no-content-hash -d ./ --out-file legacy.css",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf index.css legacy.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "autoprefixer": "^9.6.0",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "parcel-bundler": "^1.12.3",
    "postcss-css-variables": "^0.13.0",
    "prettier": "^1.18.2",
    "sass": "^1.22.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.2",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.8.0"
  },
  "dependencies": {
    "highcharts": "^7.1.2",
    "highcharts-tree": "^0.1.3",
    "normalize.css": "^8.0.1"
  }
}
