{
  "name": "@rqjs/rqthemes",
  "version": "0.7.6",
  "description": "ricequant themes",
  "main": "index.css",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.ricequant.com:7999/node/ricequant-themes.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": " parcel ./demo/index.html --out-dir demo/dist --cache-dir demo/.cache",
    "build": "./scripts/updep.sh && npm run parcel -- -d ./ && LEGACY=true npm run parcel -- -d ./ --out-file legacy.css",
    "parcel": "parcel build src/index.scss --no-cache --no-minify --no-source-maps --no-content-hash",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf index.css legacy.css light.json demo/dist demo/.cache",
    "debug": "cat legacy.css| awk '/undefinded/{getline; print}' | sort | uniq",
    "release": "standard-version --no-verify",
    "prepublishOnly": "npm run build && npm run debug"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "index.css",
    "legacy.css",
    "light.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@rqjs/rqcomponents": "0.0.6-alpha.55",
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.1",
    "element-ui": "^2.12.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "parcel": "^1.12.3",
    "postcss-css-variables": "^0.13.0",
    "prettier": "^1.18.2",
    "sass": "^1.22.9",
    "standard-version": "^9.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.10.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "highcharts": "^7.2.0",
    "highcharts-tree": "^0.1.4",
    "normalize.css": "^8.0.1",
    "vue-hot-reload-api": "^2.3.4"
  }
}
