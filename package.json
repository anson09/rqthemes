{
  "name": "@rqjs/rqthemes",
  "version": "2.0.5",
  "description": "ricequant themes",
  "main": "lib/index.css",
  "style": "lib/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/anson09/rqthemes.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "./scripts/updep.sh && parcel ./demo/index.html --out-dir demo/dist --cache-dir demo/.cache",
    "build": "./scripts/make.sh",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf demo/dist demo/.cache",
    "release": "standard-version --no-verify",
    "prepare": "husky install",
    "prepack": "./scripts/hook.sh --disable && npm run build",
    "postpack": "./scripts/hook.sh --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@vue/component-compiler-utils": "^3.2.2",
    "autoprefixer": "^9.8.8",
    "element-ui": "^2.15.6",
    "highcharts": "^9.2.2",
    "husky": "^7.0.2",
    "lint-staged": "^12.1.2",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.3.11",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "standard-version": "^9.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "vue": "^2.6.14",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.14"
  }
}
