{
  "name": "@rqjs/rqthemes",
  "version": "0.7.7",
  "description": "ricequant themes",
  "main": "lib/index.css",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.ricequant.com:7999/node/ricequant-themes.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./demo/index.html --out-dir demo/dist --cache-dir demo/.cache",
    "build": "./scripts/make.sh",
    "postbuild": "./scripts/postbuild.sh",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf demo/dist demo/.cache",
    "debug": "cat lib/legacy.css| awk '/undefinded/{getline; print}' | sort | uniq",
    "release": "standard-version --no-verify",
    "prepublishOnly": "npm run build && npm run debug"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "parcel": "^1.12.4",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "standard-version": "^9.0.0",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "element-ui": "^2.13.2",
    "highcharts": "^8.2.0",
    "highcharts-tree": "^0.1.5-alpha.7",
    "vue-hot-reload-api": "^2.3.4"
  }
}
