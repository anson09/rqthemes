{
  "name": "@rqjs/rqthemes",
  "version": "2.0.0-next.2",
  "description": "ricequant themes",
  "main": "lib/index.css",
  "style": "lib/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/anson09/rqthemes.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "./scripts/updep.sh && parcel ./demo/index.html --out-dir demo/dist --cache-dir demo/.cache",
    "build": "./scripts/make.sh",
    "lint": "./scripts/lint.sh",
    "clean": "rm -rf demo/dist demo/.cache",
    "release": "standard-version --no-verify",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run build",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "element-ui": "^2.15.0",
    "highcharts": "^9.0.1",
    "highcharts-tree": "^0.1.5-alpha.7",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "parcel": "^1.12.4",
    "pinst": "^2.1.4",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "standard-version": "^9.1.1",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.12"
  }
}
